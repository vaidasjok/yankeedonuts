<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="turbolinks-root" content="/app">
    <title> <%= yield(:title) %> || YankeeDonuts.ie</title>
    <meta name="description" content="<%= yield(:description) %>" >
    <meta name="keywords" content="<%= yield(:keywords) %>" >
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= yield(:css) %>

    <!--[if lt IE 9]>
      <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/r29/html5.min.js">
      </script>
    <![endif]-->
  </head>

  <body class="all <%= yield(:body_class) %>" >
    <div id="wrap">
      
        <section id="header">
          <div class="header-inner">
            <nav>
              <%= render('shared/menu') %>
            </nav>
              
            <a href="/"><div id="logo"></div></a>
            <a href="https://www.just-eat.ie/restaurants-yankeedonuts-rathmines/menu" id="delivery">Delivery</a>
          </div>
          <div id="juostele_po_meniu">
            <div id="star1" class="star"></div>
            <div id="star2" class="star"></div>
            <div id="star3" class="star"></div>
            <div id="star4" class="star"></div>
            <div id="star5" class="star"></div>
            <div id="star6" class="star"></div>
            <div id="star7" class="star"></div>
            <div id="star8" class="star"></div>
          </div>  
  

        </section>

      <div id="top1">
        <div id="main-image">
        </div>
        <% flash.each do |message_type, message| %>
            <div class="alert alert-<%= message_type %>"><%= message %></div>
          <% end %>
      </div><!-- #top1 -->


      <section class="content">
        <%= yield %>
      </section>

      <!-- menu items -->
      <div class="menu-item">
        <div class="menu-item-left">

        </div>
        <div class="menu-item-right">

        </div>
      </div>
     

      <!-- #menu items -->


      <!-- prideti footer ir scripts -->

      <%= render('layouts/footer.html.erb') %>

      <%= debug(params) if Rails.env.development? %>
      <script src="https://use.fontawesome.com/4b67208f55.js"></script>
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
      <%= csrf_meta_tags %>
    </div><!-- #wrap -->
  </body>

</html>
