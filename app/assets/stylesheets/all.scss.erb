@mixin links ($link, $visited, $hover, $active) {
    & {
        color: $link;
        &:visited {
            color: $visited;
        }
        &:hover {
            color: $hover;
        }
        &:active, &:focus {
            color: $active;
        }
    }
}

a:link, a:visited {
  color: #fff;
  text-decoration: none;
}
a:hover, a:active {
  background-color: none;
}


