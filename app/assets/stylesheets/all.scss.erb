}
body.all.menu {
  #header {
    margin-bottom: 0px;
  }
  #main-image {
    margin-top: 0px;
    @include container(1024px);
    padding-top: 45.8984375%;
    display: block;
    background: transparent url('<%= asset_path 'menu_main_image.jpg' %>') no-repeat center center;
    background-size: contain;
  }
  h1 {
    color: #1e173a;
    font-size: 40px;
    font-family: "Futura Bk BT Book";
    text-transform: uppercase;
    margin-top: 27px;
  }
}
section.content {
  @include container(1024px);
  padding: 0px 10px 40px;
}
body.contact {
  section.content {
    a:link, a:visited {
      color: #170f34;
    }
    a:hover, a:active {
      color: #fff;
    }
  }
}

.contact-wrapper {
  padding: 20px 10px;
}

#contact-map {
  @include span(12 of 12);
  min-height: 50px;
  background-color: transparent;
  margin-bottom: 30px;
}


/***********FORMA*****************/
.contact-left, .contact-right {
  @include span(6 of 12);
  @include susy-breakpoint(0px 677px) {
    @include span(12 of 12);
  }
}
.contact-right {
  @include susy-breakpoint(0px 677px) {
    margin-top: 30px;
  }
}
@include susy-breakpoint(678px) {
  .contact-right {
    @include last;
  }
}
.contact-left {
  padding-right: 20px;
}
.field-element {
  font-size: 14px;
  padding: 12px;
  border-radius: 2px;
}
input[type=submit] {
  padding: 10px 30px;
  background-color: transparent;
  border: 2px solid #000;
  width: initial;
  text-transform: uppercase;
  &:hover {
    background-color: #000;
    color: #fff;
  }
}
.first_name_group {
  width: 48%;
  margin-right: 4%;
  float: left;
}
.last_name_group {
  width: 48%;
  margin-right: 0%;
  float: left;
}
input:focus, textarea:focus {
    background-color: #fafafa;
}
label {
  margin-bottom: 5px;
}

/*********END FORMA***************/

/* miscellaneous */

.debug_dump {
  clear: both;
  float: left;
  width: 100%;
  margin-top: 45px;

}
.clearfix {
  @include susy-clearfix;
}
.alert {
  display: block;
  @include container(1024px);
  padding: 20px;
  margin-top: 20px;
  background-color: #dff0d8;
}
.spacer {
  margin-top: 100px;
  @include susy-breakpoint(0px 862px) {
    margin-top: 30px;
  }
}

body.all.menu, body.all.contact, body.events {
  #header {
    @include susy-breakpoint(0px 420px) {
      height: 116px;
    }
  }
}
